<?xml version="1.0" encoding="utf-8"?>
<MotionScene 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="1000">
       <KeyFrameSet>
       </KeyFrameSet>
    </Transition>

    <ConstraintSet android:id="@+id/start">
        <Constraint
            android:layout_height="126dp"
            motion:layout_constraintBottom_toBottomOf="@+id/imageView"
            android:layout_width="414dp"
            android:id="@+id/inicial"
            motion:layout_constraintTop_toTopOf="@+id/imageView"
            motion:layout_editor_absoluteY="16dp"
            motion:layout_constraintEnd_toEndOf="@+id/imageView"
            motion:layout_constraintStart_toStartOf="parent" />
        <Constraint
            motion:layout_editor_absoluteY="20dp"
            motion:layout_editor_absoluteX="20dp"
            android:id="@+id/album"
            android:layout_height="114dp"
            android:layout_width="87dp" />
        <Constraint
            android:layout_width="414dp"
            android:layout_height="126dp"
            android:id="@+id/inicial"
            motion:layout_constraintTop_toTopOf="@+id/imageView"
            motion:layout_editor_absoluteY="16dp"
            motion:layout_constraintEnd_toEndOf="@+id/imageView"
            motion:layout_constraintStart_toStartOf="parent" />
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
    </ConstraintSet>
</MotionScene>